    <xs:simpleType name="locationType.Core.Vocabulary">
        <xs:restriction base="xs:string">
            <xs:enumeration value="birth">
                <xs:annotation>
                    <xs:documentation>The place of birth of the athlete.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="hometown">
                <xs:annotation>
                    <xs:documentation>The location which best describes the place of origin of the
                        athlete. Could be different from place of birth.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="residence">
                <xs:annotation>
                    <xs:documentation>The current place of residence of the athlete.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="representative">
                <xs:annotation>
                    <xs:documentation>The place an athlete is representing. Could be different from
                        birth and residence location.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="start-location">
                <xs:annotation>
                    <xs:documentation>The starting point of the event.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="stage-location">
                <xs:annotation>
                    <xs:documentation>The location of an intermediate stage of the
                        event.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="end-location">
                <xs:annotation>
                    <xs:documentation>The end-point of the event.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="training-location">
                <xs:annotation>
                    <xs:documentation>The place where the athlete trains.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="phaseStatus.Core.Vocabulary">
        <xs:restriction base="xs:string">
            <xs:enumeration value="active">
                <xs:annotation>
                    <xs:documentation>Currently active and able to play.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="injured">
                <xs:annotation>
                    <xs:documentation>Unable to play due to injury.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="disabled-list">
                <xs:annotation>
                    <xs:documentation>Unable to play due to injury. Deemed long-term.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="try-out">
                <xs:annotation>
                    <xs:documentation>Not on time, attempting to make the lineup.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="inactive">
                <xs:annotation>
                    <xs:documentation>Inactive, not playing.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="physically-unable">
                <xs:annotation>
                    <xs:documentation>Unable to play for physical reason, not necessarily
                        injury.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
