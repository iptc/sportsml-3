    <xs:simpleType name="penaltyType.Soccer.Vocabulary">
        <xs:annotation>
            <xs:documentation>Describes the type of penalty/foul was commited.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="serious-foul-play">
                <xs:annotation>
                    <xs:documentation>The player has committed a serious foul play. | 
                        This leads to a player being sent of the field (red card).</xs:documentation>
                </xs:annotation>			
            </xs:enumeration>
            <xs:enumeration value="violent-conduct">
                <xs:annotation>
                    <xs:documentation>The player has committed a violent conduct. | 
                        This leads to a player being sent off the field (red card).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="spitting"> 
                <xs:annotation>
                    <xs:documentation>The player has spitted at an opponent or any other person. |
                        This leads to a player being sent off the field (red card).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="offensive-language">
                <xs:annotation>
                    <xs:documentation>The player has used offensive, insulting or abusive language. | 
                        This leads to a player being sent off the field (red card).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="offensive-gestures">
                <xs:annotation>
                    <xs:documentation>The player has used offensive, insulting or abusive gestures. | 
                        This leads to a player being sent off the field (red card).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="professional-foul"> 
                <xs:annotation>
                    <xs:documentation>When a player denies another player an obvious goalscoring opportunity by foul. | 
                        This action leads to the player being sent off the field.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="unsporting-behaviour">
                <xs:annotation>
                    <xs:documentation>The player has been doing unsporting behaviour. | 
                        This leads to a player being given a caution (yellow card).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="delaying-restart">
                <xs:annotation>
                    <xs:documentation>The player has delayed the restart of play. | 
                        This leads to a player being given a caution (yellow card).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="illegal-reentry">
                <xs:annotation>
                    <xs:documentation>The player re-entered the ﬁeld of play without the referee’s permission. | 
                        This leads to a player being given a caution (yellow card).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="illegal-entry">
                <xs:annotation>
                    <xs:documentation>The player entered the ﬁeld of play without the referee’s permission. | 
                        This leads to a player being given a caution (yellow card).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="required-distance-foul">
                <xs:annotation>
                    <xs:documentation>The played has failed to respect the required distance when play is restarted 
                        with a corner kick, free kick or throw-in. | 
                        This leads to a player being given a caution (yellow card).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>	
            <xs:enumeration value="illegal-exit">
                <xs:annotation>
                    <xs:documentation>The player has deliberately left the field of play without the referee’s permission. | 
                        This leads to a player being given a caution (yellow card).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="hand-ball"/>
            <xs:enumeration value="tackle"/>
        </xs:restriction>
    </xs:simpleType>
    
