    <xs:simpleType name="competitionScope.Core.Vocabulary">
        <xs:restriction base="xs:string">
            <xs:enumeration value="events-all">
                <xs:annotation>
                    <xs:documentation>Every event played, including those played against teams in the
                        same division or conference. The default option.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="division">
                <xs:annotation>
                    <xs:documentation>Events played within a league's division.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="division-opposing"/>
            <xs:enumeration value="conference">
                <xs:annotation>
                    <xs:documentation>Events played within a league's conference.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="conference-opposing"/>
            <xs:enumeration value="team-opposing">
                <xs:annotation>
                    <xs:documentation>Events against a specific team.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="player-opposing">
                <xs:annotation>
                    <xs:documentation>
                        <xs:annotation>
                            <xs:documentation>Events against a specific player.</xs:documentation>
                        </xs:annotation>
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="tournament">
                <xs:annotation>
                    <xs:documentation>Events from a specific tournament.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="league"/>
            <xs:enumeration value="top-25"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="codeType.Core.Vocabulary">
        <xs:annotation>
            <xs:documentation>What type of item is being coded.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="sport-class"/>
            <xs:enumeration value="sport-category"/>
            <xs:enumeration value="sport"/>
            <xs:enumeration value="sport-competition"/>
            <xs:enumeration value="league"/>
            <xs:enumeration value="conference"/>
            <xs:enumeration value="division"/>
            <xs:enumeration value="team"/>
            <xs:enumeration value="special-event"/>
            <xs:enumeration value="site"/>
            <xs:enumeration value="article-ref"/>
            <xs:enumeration value="photo-ref"/>
            <xs:enumeration value="audio-ref"/>
            <xs:enumeration value="video-ref"/>
            <xs:enumeration value="xref-key"/>
            <xs:enumeration value="slug"/>
            <xs:enumeration value="slug-category"/>
            <xs:enumeration value="season">
                <xs:annotation>
                    <xs:documentation>The year or name of the season</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="season-type">
                <xs:annotation>
                    <xs:documentation>Which segment of the season: pre-season, post-season, regular,
                        etc.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="publisher">
                <xs:annotation>
                    <xs:documentation>The source of the data/content.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="distributor">
                <xs:annotation>
                    <xs:documentation>The source of the data/content.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="priority"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="eventStatus.Core.Vocabulary">
        <xs:annotation>
            <xs:documentation>pre-event, mid-event, post-event.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="pre-event">
                <xs:annotation>
                    <xs:documentation>Any time before an event begins.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="mid-event">
                <xs:annotation>
                    <xs:documentation>Any time while an event is in-play.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="post-event">
                <xs:annotation>
                    <xs:documentation>Any time after an event is finished.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="postponed">
                <xs:annotation>
                    <xs:documentation>The putting off of an event until a later date, before the event
                        has begun, for whatever reason.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="suspended">
                <xs:annotation>
                    <xs:documentation>An event is stopped with the intention of resumption at a later
                        date.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="halted">
                <xs:annotation>
                    <xs:documentation>An event is stopped and not resumed or ended.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="forfeited">
                <xs:annotation>
                    <xs:documentation>A team declines or is unable to play and loses by default. The
                        opponent is awarded the win.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="rescheduled">
                <xs:annotation>
                    <xs:documentation>An event is given a new time and date.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="delayed">
                <xs:annotation>
                    <xs:documentation>An event is held up, for example by inclement weather. Can happen
                        before or during an event.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="canceled">
                <xs:annotation>
                    <xs:documentation>An event is never played and not re-scheduled.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="intermission">
                <xs:annotation>
                    <xs:documentation>A planned or scheduled break in play after which play
                        resumes.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="if-necessary">
                <xs:annotation>
                    <xs:documentation>An event whose commencement is dependant upon the outcome of
                        prior events.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="discarded">
                <xs:annotation>
                    <xs:documentation>A cancelled event because the event was
                        unnecessary.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="seasonType.Core.Vocabulary">
        <xs:annotation>
            <xs:documentation>A subcategory of the season</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="pre-season">
                <xs:annotation>
                    <xs:documentation>Events played in an exhibition before the regular phase when
                        standings points are awarded.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="season-regular">
                <xs:annotation>
                    <xs:documentation>The phase of a league competition when standings points are
                        awarded toward a points-based championship or position in a post-season
                        phase.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="post-season">
                <xs:annotation>
                    <xs:documentation>A championship phase of a league competition, usually comprosed
                        of elimination rounds until one team has prevailed.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="all-star-game">
                <xs:annotation>
                    <xs:documentation>Some leagues have an all-star break and game comprised of the
                        best players.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="exhibition">
                <xs:annotation>
                    <xs:documentation>A game or phase with no standings points awarded.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="recordMakingScope.Core.Vocabulary">
        <xs:restriction base="xs:string">
            <xs:enumeration value="high"/>
            <xs:enumeration value="low"/>
        </xs:restriction>
    </xs:simpleType>
    
    
    <xs:simpleType name="status.Core.Vocabulary">
        <xs:annotation>
            <xs:documentation>Whether a player starts playing at the beginning of a sports-event, joins
                mid-game, or is not available to participate.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="starter">
                <xs:annotation>
                    <xs:documentation>A member of the lineup that enters the field at the commencement
                        of play.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="bench">
                <xs:annotation>
                    <xs:documentation>Does not enter field at commencement of play, is substituted in
                        or remains on bench.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="scratched">
                <xs:annotation>
                    <xs:documentation>Does not dress for event, usually for having not earned a slot in
                        the lineup.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="injured">
                <xs:annotation>
                    <xs:documentation>Unable to play due to injury.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="suspended">
                <xs:annotation>
                    <xs:documentation>Unable to play due to suspension due to
                        transgression.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="sidelined">
                <xs:annotation>
                    <xs:documentation>Does not play for other reason.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="health.Core.Vocabulary">
        <xs:annotation>
            <xs:documentation>An indication of the health of the player.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="injured"/>
            <xs:enumeration value="fine"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="specialGroup.Core.Vocabulary">
