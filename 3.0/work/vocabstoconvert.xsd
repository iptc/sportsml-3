    <xs:simpleType name="scoreUnits.Core.Vocabulary">
        <xs:annotation>
            <xs:documentation>Describes how the score and score-opposing is valued.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="time-absolute">
                <xs:annotation>
                    <xs:documentation>Total time to complete competition, not measured in comparison to
                        other competitors</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="time-relative">
                <xs:annotation>
                    <xs:documentation>Time difference compared (usually) to closest competitor or
                        first-place finalist. First place commonly has absolute
                        value.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="against-par"/>
        </xs:restriction>
    </xs:simpleType>
    
