    <conceptSet>
      <concept id="spsocscoreattemptregular">
        <conceptId qcode="spsocscoreattempt:regular"/>
        <name xml:lang="en-GB">regular</name>
        <definition>The score attempt is made during regular play.</definition>
        <related uri="http://cv.iptc.org/newscodes/spsocscoreattempt/" rel="skos:inScheme"/>
      </concept>
      <concept id="spsocscoreattemptpenalty">
        <conceptId qcode="spsocscoreattempt:penalty"/>
        <name xml:lang="en-GB">penalty</name>
        <definition>The score attempt is made as a penalty shot.</definition>
        <related uri="http://cv.iptc.org/newscodes/spsocscoreattempt/" rel="skos:inScheme"/>
      </concept>
      <concept id="spsocscoreattemptfreekick">
        <conceptId qcode="spsocscoreattempt:freekick"/>
        <name xml:lang="en-GB">freekick</name>
        <definition>The score attempt is made as a free kick.</definition>
        <related uri="http://cv.iptc.org/newscodes/spsocscoreattempt/" rel="skos:inScheme"/>
      </concept>
      <concept id="spsocscoreattemptcorner-kick">
        <conceptId qcode="spsocscoreattempt:corner-kick"/>
        <name xml:lang="en-GB">corner-kick</name>
        <definition>The score attempt is made as a kick from the corner.</definition>
        <related uri="http://cv.iptc.org/newscodes/spsocscoreattempt/" rel="skos:inScheme"/>
      </concept>
      <concept id="spsocscoreattemptopen-goal">
        <conceptId qcode="spsocscoreattempt:open-goal"/>
        <name xml:lang="en-GB">open-goal</name>
