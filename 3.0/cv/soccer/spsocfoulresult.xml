      <conceptSet>
         <concept id="spsocfoulresultplay-on">
            <conceptId qcode="spsocfoulresult:play-on"/>
            <name xml:lang="en-GB">play-on</name>
            <definition>Fouled team has advantage of the ball and is allowed to play
						on.</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocfoulresult/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocfoulresultfreekick">
            <conceptId qcode="spsocfoulresult:freekick"/>
            <name xml:lang="en-GB">freekick</name>
            <definition>Fouled team is awarded a freekick.</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocfoulresult/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocfoulresultindirect-freekick">
            <conceptId qcode="spsocfoulresult:indirect-freekick"/>
            <name xml:lang="en-GB">indirect-freekick</name>
            <definition>Fouled team is awarded a freekick.</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocfoulresult/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocfoulresultdirect-freekick">
            <conceptId qcode="spsocfoulresult:direct-freekick"/>
            <name xml:lang="en-GB">direct-freekick</name>
            <definition>Fouled team is awarded a freekick.</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocfoulresult/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocfoulresultpenalty-shot">
            <conceptId qcode="spsocfoulresult:penalty-shot"/>
            <name xml:lang="en-GB">penalty-shot</name>
            <definition>The foul occured in the penalty-shot area so the fouled team is
						awarded a penalty-shot.</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocfoulresult/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocfoulresultcaution">
            <conceptId qcode="spsocfoulresult:caution"/>
            <name xml:lang="en-GB">caution</name>
            <definition>The player is beeing cautioned. Indicated by a yellow card.
						Suffered team get a freekick or penalty-shot.</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocfoulresult/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocfoulresultejection">
            <conceptId qcode="spsocfoulresult:ejection"/>
            <name xml:lang="en-GB">ejection</name>
            <definition>The player is beeing ejected from the game. Shown by a red card.
						Suffered team get a freekick or penalty shot.</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocfoulresult/" rel="skos:inScheme"/>
         </concept>
      </conceptSet>
