      <conceptSet>
         <concept id="spsocpenaltyserious-foul-play">
            <conceptId qcode="spsocpenalty:serious-foul-play"/>
            <name xml:lang="en-GB">serious-foul-play</name>
            <definition>The player has committed a serious foul play. | 
					This leads to a player being sent of the field (red card).</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltyviolent-conduct">
            <conceptId qcode="spsocpenalty:violent-conduct"/>
            <name xml:lang="en-GB">violent-conduct</name>
            <definition>The player has committed a violent conduct. | 
						This leads to a player being sent off the field (red card).</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltyspitting">
            <conceptId qcode="spsocpenalty:spitting"/>
            <name xml:lang="en-GB">spitting</name>
            <definition>The player has spitted at an opponent or any other person. |
						This leads to a player being sent off the field (red card).</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltyoffensive-language">
            <conceptId qcode="spsocpenalty:offensive-language"/>
            <name xml:lang="en-GB">offensive-language</name>
            <definition>The player has used offensive, insulting or abusive language. | 
						This leads to a player being sent off the field (red card).</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltyoffensive-gestures">
            <conceptId qcode="spsocpenalty:offensive-gestures"/>
            <name xml:lang="en-GB">offensive-gestures</name>
            <definition>The player has used offensive, insulting or abusive gestures. | 
						This leads to a player being sent off the field (red card).</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltyprofessional-foul">
            <conceptId qcode="spsocpenalty:professional-foul"/>
            <name xml:lang="en-GB">professional-foul</name>
            <definition>When a player denies another player an obvious goalscoring opportunity by foul. | 
						This action leads to the player being sent off the field.</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltyunsporting-behaviour">
            <conceptId qcode="spsocpenalty:unsporting-behaviour"/>
            <name xml:lang="en-GB">unsporting-behaviour</name>
            <definition>The player has been doing unsporting behaviour. | 
						This leads to a player being given a caution (yellow card).</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltydelaying-restart">
            <conceptId qcode="spsocpenalty:delaying-restart"/>
            <name xml:lang="en-GB">delaying-restart</name>
            <definition>The player has delayed the restart of play. | 
						This leads to a player being given a caution (yellow card).</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltyillegal-reentry">
            <conceptId qcode="spsocpenalty:illegal-reentry"/>
            <name xml:lang="en-GB">illegal-reentry</name>
            <definition>The player re-entered the ﬁeld of play without the referee’s permission. | 
						This leads to a player being given a caution (yellow card).</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltyillegal-entry">
            <conceptId qcode="spsocpenalty:illegal-entry"/>
            <name xml:lang="en-GB">illegal-entry</name>
            <definition>The player entered the ﬁeld of play without the referee’s permission. | 
						This leads to a player being given a caution (yellow card).</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltyrequired-distance-foul">
            <conceptId qcode="spsocpenalty:required-distance-foul"/>
            <name xml:lang="en-GB">required-distance-foul</name>
            <definition>The played has failed to respect the required distance when play is restarted 
						with a corner kick, free kick or throw-in. | 
						This leads to a player being given a caution (yellow card).</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltyillegal-exit">
            <conceptId qcode="spsocpenalty:illegal-exit"/>
            <name xml:lang="en-GB">illegal-exit</name>
            <definition>The player has deliberately left the field of play without the referee’s permission. | 
						This leads to a player being given a caution (yellow card).</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltyhand-ball">
            <conceptId qcode="spsocpenalty:hand-ball"/>
            <name xml:lang="en-GB">hand-ball</name>
            <definition/>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltytackle">
            <conceptId qcode="spsocpenalty:tackle"/>
            <name xml:lang="en-GB">tackle</name>
            <definition/>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltyoffensive">
            <conceptId qcode="spsocpenalty:offensive"/>
            <name xml:lang="en-GB">offensive</name>
            <definition>Deprecated: use new penaltySide vocab.</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltydefensive">
            <conceptId qcode="spsocpenalty:defensive"/>
            <name xml:lang="en-GB">defensive</name>
            <definition>Deprecated: use new penaltySide vocab.</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
         <concept id="spsocpenaltyrough-play">
            <conceptId qcode="spsocpenalty:rough-play"/>
            <name xml:lang="en-GB">rough-play</name>
            <definition>The player has been penalized for rough play.</definition>
            <related uri="http://cv.iptc.org/newscodes/spsocpenalty/" rel="skos:inScheme"/>
         </concept>
