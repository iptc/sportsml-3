         <subject qcode="subj:15000000">
            <name xml:lang="en-US">sport</name>
         </subject>
         <description>Whether a player starts playing at the beginning of a sports-event, joins
				mid-game, or is not available to participate.</description>
      </contentMeta>
      <conceptSet>
         <concept id="spstatusstarter">
            <conceptId qcode="spstatus:starter"/>
            <name xml:lang="en-GB">starter</name>
            <definition>A member of the lineup that enters the field at the commencement
						of play.</definition>
            <related uri="http://cv.iptc.org/newscodes/spstatus/" rel="skos:inScheme"/>
         </concept>
         <concept id="spstatusbench">
            <conceptId qcode="spstatus:bench"/>
            <name xml:lang="en-GB">bench</name>
            <definition>Does not enter field at commencement of play, is substituted in
						or remains on bench.</definition>
            <related uri="http://cv.iptc.org/newscodes/spstatus/" rel="skos:inScheme"/>
         </concept>
         <concept id="spstatusscratched">
            <conceptId qcode="spstatus:scratched"/>
            <name xml:lang="en-GB">scratched</name>
            <definition>Does not dress for event, usually for having not earned a slot in
						the lineup.</definition>
            <related uri="http://cv.iptc.org/newscodes/spstatus/" rel="skos:inScheme"/>
         </concept>
         <concept id="spstatusinjured">
            <conceptId qcode="spstatus:injured"/>
            <name xml:lang="en-GB">injured</name>
            <definition>Unable to play due to injury.</definition>
            <related uri="http://cv.iptc.org/newscodes/spstatus/" rel="skos:inScheme"/>
         </concept>
         <concept id="spstatussuspended">
            <conceptId qcode="spstatus:suspended"/>
            <name xml:lang="en-GB">suspended</name>
            <definition>Unable to play due to suspension due to
						transgression.</definition>
            <related uri="http://cv.iptc.org/newscodes/spstatus/" rel="skos:inScheme"/>
         </concept>
         <concept id="spstatussidelined">
            <conceptId qcode="spstatus:sidelined"/>
            <name xml:lang="en-GB">sidelined</name>
            <definition>Does not play for other reason.</definition>
            <related uri="http://cv.iptc.org/newscodes/spstatus/" rel="skos:inScheme"/>
         </concept>
      </conceptSet>
