    <contentMeta>
        <contentCreated>2014-11-03T15:25:15+00:00</contentCreated>
        <description xml:lang="en-US">General sports-related concepts such as team, player,
            etc.</description>
    </contentMeta>
    <conceptSet>
        <concept id="spctsport">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:sport"/>
            <name xml:lang="en-US">Sport</name>
            <definition xml:lang="en-US"/>
        </concept>
        <concept id="spctleague">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:league"/>
            <name xml:lang="en-US">League</name>
            <definition xml:lang="en-US"/>
        </concept>
        <concept id="spctseason">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:season"/>
            <name xml:lang="en-US">Season</name>
            <definition xml:lang="en-US"/>
        </concept>
        <concept id="spctseason-type">
            <conceptId qcode="spct:season-type"/>
            <name xml:lang="en-US">season-type</name>
            <definition>Which segment of the season: pre-season, post-season, regular,
                etc.</definition>
            <related uri="http://cv.iptc.org/newscodes/spct/" rel="skos:inScheme"/>
        </concept>
        <concept id="spctseason-regular">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:season-regular"/>
            <name xml:lang="en-US">Regular Season</name>
            <definition xml:lang="en-US">The phase of a season during which competitors vie for
                placement in a playoff phase.</definition>
        </concept>
        <concept id="spctpost-season">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:post-season"/>
            <name xml:lang="en-US">Post Season</name>
            <definition xml:lang="en-US">The phase of a season after the regular season during which
                competitors vie for the league championship.</definition>
        </concept>
        <concept id="spctpre-season">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:pre-season"/>
            <name xml:lang="en-US">Pre Season</name>
            <definition xml:lang="en-US">The phase of a season before the regular season during
                which exhibition matches are played.</definition>
        </concept>
        <concept id="spctcompetition">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:competition"/>
            <name xml:lang="en-US">Competition</name>
            <definition xml:lang="en-US"/>
        </concept>
        <concept id="spctconference">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:conference"/>
            <name xml:lang="en-US">Conference</name>
            <definition xml:lang="en-US"/>
        </concept>
        <concept id="spctdivision">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:division"/>
            <name xml:lang="en-US">Division</name>
            <definition xml:lang="en-US"/>
        </concept>
        <concept id="spctnews">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:news"/>
            <name xml:lang="en-US">News</name>
            <definition xml:lang="en-US">A category of sports documents: non-event-specific news
                reports for player, team, league, etc.</definition>
        </concept>
        <concept id="spctstandings">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:standings"/>
            <name xml:lang="en-US">Standings</name>
            <definition xml:lang="en-US">A category of sports documents: often also called
                "tables"</definition>
        </concept>
        <concept id="spctschedules">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:schedules"/>
            <name xml:lang="en-US">Schedules</name>
            <definition xml:lang="en-US">A category of sports documents.</definition>
        </concept>
        <concept id="spctstatistics">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:statistics"/>
            <name xml:lang="en-US">Statistics</name>
            <definition xml:lang="en-US">A category of sports documents: for non-event-specific
                statistical reports</definition>
        </concept>
        <concept id="spctevent-summary">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:event-summary"/>
            <name xml:lang="en-US">Event Summary</name>
            <definition xml:lang="en-US">A category of sports documents: for event-specific reports
                whether prose (i.e. previews and reviews) or statistical (i.e.
                boxscores))</definition>
        </concept>
        <concept id="spctevent-action">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:event-action"/>
            <name xml:lang="en-US">Event Action</name>
            <definition xml:lang="en-US">A category of sports documents: for play actions within a
                single event</definition>
        </concept>
        <concept id="spctdynamic-resource-file">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:dynamic-resource-file"/>
            <name xml:lang="en-US">Dynamic Resource File</name>
            <definition xml:lang="en-US">A category of sports documents: for team rosters (squad
                lists), athlete lists, league groupings and teams, etc.</definition>
        </concept>
        <concept id="spctteam">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:team"/>
            <name xml:lang="en-US">Team</name>
            <definition xml:lang="en-US"/>
        </concept>
        <concept id="spctstat">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:stat"/>
            <name xml:lang="en-US">Statistic</name>
            <definition xml:lang="en-US">A statistical indicator of on-field performance in
                sport.</definition>
        </concept>
        <concept id="spctoffense">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:offense"/>
            <name xml:lang="en-US">Offense</name>
            <definition xml:lang="en-US">A class of position, role or statistic in
                sport.</definition>
        </concept>
        <concept id="spctdefense">
            <conceptId created="2013-01-02T01:00:00+00:00" qcode="spct:defense"/>
            <name xml:lang="en-US">Defense</name>
            <definition xml:lang="en-US">A class of position, role or statistic in
                sport.</definition>
        </concept>
