        <concept id="spmcrstatspeed-average">
            <conceptId qcode="spmcrstat:speed-average"/>
            <type qcode="spct:general"/>
            <name xml:lang="en-GB">speed-average</name>
            <definition>Average speed during the race.</definition>
            <related uri="http://cv.iptc.org/newscodes/spmcrstat/" rel="skos:inScheme"/>
