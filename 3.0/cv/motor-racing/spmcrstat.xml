            <related uri="http://cv.iptc.org/newscodes/spmcrstat/" rel="skos:inScheme"/>
        </concept>
        <concept id="spmcrstatcomment">
            <conceptId qcode="spmcrstat:comment"/>
            <type qcode="spct:general"/>
            <name xml:lang="en-GB">comment</name>
