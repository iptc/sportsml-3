            <related uri="http://cv.iptc.org/newscodes/spmcrstat/" rel="skos:inScheme"/>
        </concept>
        <concept id="spmcrstatstart-time-elapsed">
            <conceptId qcode="spmcrstat:start-time-elapsed"/>
            <type qcode="spct:general"/>
            <name xml:lang="en-GB">start-time-elapsed</name>
            <definition>The time since the start of the race at which this lap was
