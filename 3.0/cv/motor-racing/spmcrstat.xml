            <related uri="http://cv.iptc.org/newscodes/spmcrstat/" rel="skos:inScheme"/>
        </concept>
        <concept id="spmcrstatspeed-average-lap">
            <conceptId qcode="spmcrstat:speed-average-lap"/>
            <type qcode="spct:general"/>
            <name xml:lang="en-GB">speed-average-lap</name>
            <definition>The average speed of this lap.</definition>
            <related uri="http://cv.iptc.org/newscodes/spmcrstat/" rel="skos:inScheme"/>
