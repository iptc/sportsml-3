            <conceptId qcode="spmcrstat:speed-range"/>
            <type qcode="spct:general"/>
            <name xml:lang="en-GB">speed-range</name>
            <definition>Either fastest or slowest.</definition>
            <related uri="http://cv.iptc.org/newscodes/spmcrstat/" rel="skos:inScheme"/>
        </concept>
        <concept id="spmcrstatlap-number">
            <conceptId qcode="spmcrstat:lap-number"/>
            <type qcode="spct:general"/>
            <name xml:lang="en-GB">lap-number</name>
