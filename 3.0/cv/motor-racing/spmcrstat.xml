            <conceptId qcode="spmcrstat:money"/>
            <type qcode="spct:general"/>
            <name xml:lang="en-GB">money</name>
            <definition>Amount of money this driver had won.</definition>
            <related uri="http://cv.iptc.org/newscodes/spmcrstat/" rel="skos:inScheme"/>
        </concept>
        <concept id="spmcrstatleads-total">
            <conceptId qcode="spmcrstat:leads-total"/>
            <type qcode="spct:general"/>
            <name xml:lang="en-GB">leads-total</name>
            <definition>Number of total leads this driver has had, presumably during this
