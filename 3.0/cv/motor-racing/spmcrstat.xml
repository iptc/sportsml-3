        <concept id="spmcrstatfinishes">
            <conceptId qcode="spmcrstat:finishes"/>
            <type qcode="spct:general"/>
            <name xml:lang="en-GB">finishes</name>
            <definition>Number of races this driver had finished.</definition>
            <related uri="http://cv.iptc.org/newscodes/spmcrstat/" rel="skos:inScheme"/>
