	<conceptSet>
		<concept id="spsocstatline-formation-row">
			<conceptId qcode="spsocstat:line-formation-row" created="2016-05-02T00:00:00+00:00"/>
			<name xml:lang="en-US">line-formation-row</name>
            <name xml:lang="en-GB">line-formation-row</name>
			<definition xml:lang="en-GB">Integer value ascending from closest to the goalkeeper. All players in the
				row closest to the goalkeeper have 1 as value, the next row 2, etc. Indicates row
				the player belonged to at the start of the game or when they entered the
				game.</definition>
			<related uri="http://cv.iptc.org/newscodes/spsocstat/" rel="skos:inScheme"/>
		</concept>
