        <description>Types of shot in curling.</description>
    </contentMeta>
    <conceptSet>
        <concept id="spcurshotdraw">
            <conceptId qcode="spcurshot:draw"/>
            <name xml:lang="en-US">draw</name>
            <definition>A shot trying to place the stone inside the house.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshotcome-around">
            <conceptId qcode="spcurshot:come-around"/>
            <name xml:lang="en-US">come-around</name>
            <definition>A shot trying to move the stone around another stone placing the new stone
                inside the house.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshotfront">
            <conceptId qcode="spcurshot:front"/>
            <name xml:lang="en-US">front</name>
            <definition>A shot with the intention of placing the stone in front of the
                house.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshotguard">
            <conceptId qcode="spcurshot:guard"/>
            <name xml:lang="en-US">guard</name>
            <definition>A shot that is placed in front of another stone to protect it from being
                knocked out by the other team, or placed with the intent to later curl another rock
                around it and thus be protected; typically placed between the hog line and the very
                front of the house.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshotraise">
            <conceptId qcode="spcurshot:raise"/>
            <name xml:lang="en-US">raise</name>
            <definition>Using one stone to hit another stone with the intention of moving it
                forward.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshotfreeze">
            <conceptId qcode="spcurshot:freeze"/>
            <name xml:lang="en-US">freeze</name>
            <definition>A precise draw weight shot where a delivered stone comes to rest against a
                stationary stone, making it nearly impossible to knock out.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshotsplit">
            <conceptId qcode="spcurshot:split"/>
            <name xml:lang="en-US">split</name>
            <definition>A draw shot in which the played stone hits on the side of a stationary stone
                and both move sideways and stay in play.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshottick">
            <conceptId qcode="spcurshot:tick"/>
            <name xml:lang="en-US">tick</name>
            <definition>A shot that bumps a guard out of the way without removing it from
                play.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshottakeout">
            <conceptId qcode="spcurshot:takeout"/>
            <name xml:lang="en-US">takeout</name>
            <definition>A shot that tries to hit and remove another stone from the
                game.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshothit-and-roll">
            <conceptId qcode="spcurshot:hit-and-roll"/>
            <name xml:lang="en-US">hit-and-roll</name>
            <definition>A shot that tries to hit and remove another stone from the game and then
                moving the shot stone sideways.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshotclearing">
            <conceptId qcode="spcurshot:clearing"/>
            <name xml:lang="en-US">clearing</name>
            <definition>A shot that tries to hit and remove (an)other stone(s) including the shot
                stone.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshotdouble">
            <conceptId qcode="spcurshot:double"/>
            <name xml:lang="en-US">double</name>
            <definition>A shot that tries to hit and remove two other stones.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshotraise-takeout">
            <conceptId qcode="spcurshot:raise-takeout"/>
            <name xml:lang="en-US">raise-takeout</name>
            <definition>A shot that tries to hit another stone that in turn should hit a third stone
                removing that stone from play.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshotnot-considered">
            <conceptId qcode="spcurshot:not-considered"/>
            <name xml:lang="en-US">not-considered</name>
            <definition>A shot that is not to be considered in the statistics for the
                game.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshotteam-fault">
            <conceptId qcode="spcurshot:team-fault"/>
            <name xml:lang="en-US">team-fault</name>
            <definition>A shot that the team made something wrong with like touching the stone after
                release etc.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
        <concept id="spcurshotblank">
            <conceptId qcode="spcurshot:blank"/>
            <name xml:lang="en-US">blank</name>
            <definition>A stone that is not played at all, for example all remaining stones in the
                game when one team has given up the game.</definition>
            <related uri="http://cv.iptc.org/newscodes/spcurshot/" rel="skos:inScheme"/>
        </concept>
    </conceptSet>
