        <description>Types of scoring in ice-hockey.</description>
    </contentMeta>
    <conceptSet>
        <concept id="spichscoreattemptregular">
            <conceptId qcode="spichscoreattempt:regular"/>
            <name xml:lang="en-US">regular</name>
            <definition>Occurs while both teams have an equal number of players on the
                ice.</definition>
            <related uri="http://cv.iptc.org/newscodes/spichscoreattempt/" rel="skos:inScheme"/>
        </concept>
        <concept id="spichscoreattemptpenalty-shot">
            <conceptId qcode="spichscoreattempt:penalty-shot"/>
            <name xml:lang="en-US">penalty-shot</name>
            <definition>Occurs while shooting player is taking a penalty shot.</definition>
            <related uri="http://cv.iptc.org/newscodes/spichscoreattempt/" rel="skos:inScheme"/>
        </concept>
        <concept id="spichscoreattemptempty-net">
            <conceptId qcode="spichscoreattempt:empty-net"/>
            <name xml:lang="en-US">empty-net</name>
            <definition>Occurs when shot is taken on net vacated by goalie who has been replaced on
                the ice by a non-goalie.</definition>
            <related uri="http://cv.iptc.org/newscodes/spichscoreattempt/" rel="skos:inScheme"/>
        </concept>
        <concept id="spichscoreattemptshootout">
            <conceptId qcode="spichscoreattempt:shootout"/>
            <name xml:lang="en-US">shootout</name>
            <definition>Occurs when a shot is taken during a game-ending shootout
                situation.</definition>
            <related uri="http://cv.iptc.org/newscodes/spichscoreattempt/" rel="skos:inScheme"/>
        </concept>
        <concept id="spichscoreattemptown-goal">
            <conceptId qcode="spichscoreattempt:own-goal"/>
            <name xml:lang="en-US">own-goal</name>
            <definition>Occurs when shot is inadvertantly scored into the wrong goal.</definition>
            <related uri="http://cv.iptc.org/newscodes/spichscoreattempt/" rel="skos:inScheme"/>
        </concept>
        <concept id="spichscoreattemptawarded">
            <conceptId qcode="spichscoreattempt:awarded"/>
            <name xml:lang="en-US">awarded</name>
            <definition>Occurs when own-goals are not awarded and not opponent has contact with
                puck. Goal is awarded to non-participant on opposing team of actual
                scorer.</definition>
            <related uri="http://cv.iptc.org/newscodes/spichscoreattempt/" rel="skos:inScheme"/>
        </concept>
    </conceptSet>
